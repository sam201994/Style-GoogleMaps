{
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "indent": [
      1,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      0
    ],
    "max-len": [
      1,
      120
    ],
    "no-param-reassign": [
      0
    ],
    "no-underscore-dangle": [
      0
    ],
    "jsx-a11y/anchor-has-content": [
      0
    ],
    "react/jsx-indent": [
      1,
      4
    ],
    "react/jsx-indent-props": [
      2,
      4
    ],
    "react/prop-types": [
      0
    ]
  }
}